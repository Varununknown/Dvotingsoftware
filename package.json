{
  "name": "e-voting-system",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "start": "vite preview --host",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "test": "echo \"No tests specified\" && exit 0",
    "prepare-deploy": "npm run build:prod && echo \"Frontend build complete. Ready for deployment!\"",
    "backend": "cd backend && npm run dev",
    "frontend": "npm run dev",
    "dev:full": "concurrently \"npm run backend\" \"npm run frontend\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@walletconnect/sign-client": "^2.21.10",
    "@walletconnect/types": "^2.21.10",
    "@walletconnect/utils": "^2.21.10",
    "ethers": "^6.15.0",
    "face-api.js": "^0.22.2",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
